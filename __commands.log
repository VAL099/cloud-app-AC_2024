# PART 1:
docker build -t ac_app .
docker run -d -p 7100:8080 ac_app

# PART 2:
docker tag ac_app:latest val099/ac_app:latest
docker push val099/ac_app:latest


# PART 3:
kubectl apply -f deployment-v0.5.yaml # first
kubectl apply -f deployment-v1.yaml # custom
kubectl apply -f deployment_config-v1.yaml # as service
kubectl get service ac-laboratory-service # get_ip

# PART 4:
INSTALL gcloud

- gcloud init
- gcloud container clusters get-credentials my-cluster

# PART 5:
-> REPEAT PART 3 AFTER CONNECTING TO GKE

# PART 6:
kubectl get deployments
kubectl scale deployment cloud-app-deployment --replicas=3

# PART 7:
write into config:
                    strategy:
                    type: RollingUpdate
                    rollingUpdate:
                        maxUnavailable: 1
                        maxSurge: 1

# PART 8:
kubectl get deployments
kubectl rollout undo deployment <deployment-name>

# PART 10:
gcloud container clusters update <cluster-name> \
  --enable-autoscaling --min-nodes=1 --max-nodes=10 \
  --zone <zone> --node-pool <node-pool-name>

